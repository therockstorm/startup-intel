{
  "extends": [
    ":combinePatchMinorReleases",
    ":disableRateLimiting",
    ":enableVulnerabilityAlertsWithLabel(security)",
    ":timezone(America/Denver)",
    "config:js-app",
    "group:allNonMajor",
    "npm:unpublishSafe",
    "schedule:weekly"
  ],
  "hostRules": [
    {
      "encrypted": {
        "token": ""
      },
      "hostType": "npm",
      "matchHost": "https://npm.fontawesome.com/"
    }
  ],
  "packageRules": [
    {
      "automerge": true,
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ]
}
